{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "initialRouteName": "index",
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "linking": {
      "prefixes": ["myapp://"],
      "config": {
        "screens": {
          "index": "/",
          "(auth)": {
            "screens": {
              "login": "/login",
              "register": "/register"
            }
          },
          "(tabs)": {
            "screens": {
              "index": "/home",
              "courses": "/courses",
              "profile": "/profile",
              "leaderboard": "/leaderboard"
            }
          }
        }
      }
    }
  }
}
